{
  "name": "page-trail",
  "version": "1.0.0",
  "description": "This Extension tracks your daily surfing activity",
  "permissions": [
    "storage",
    "webNavigation",
    "tabs"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "PageTrail Pro",
    "default_popup": "index.html"
  },
  "manifest_version": 3
}